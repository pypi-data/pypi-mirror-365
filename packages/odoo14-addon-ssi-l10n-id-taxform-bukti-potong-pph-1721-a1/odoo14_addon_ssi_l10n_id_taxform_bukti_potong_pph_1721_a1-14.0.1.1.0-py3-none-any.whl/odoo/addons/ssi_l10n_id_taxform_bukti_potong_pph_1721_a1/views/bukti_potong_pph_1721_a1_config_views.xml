<odoo>
<record id="bukti_potong_pph_1721_a1_config_view_tree" model="ir.ui.view">
    <field name="name">Bukti Potong PPh 21 1721 A1 Config - Tree</field>
    <field name="model">l10n_id.bukti_potong_pph_1721_a1_config</field>
    <field name="arch" type="xml">
        <tree>
            <field name="field_id" />
        </tree>
    </field>
</record>

<record id="bukti_potong_pph_1721_a1_config_view_form" model="ir.ui.view">
    <field name="name">Bukti Potong PPh 21 1721 A1 Config - Form</field>
    <field name="model">l10n_id.bukti_potong_pph_1721_a1_config</field>
    <field name="arch" type="xml">
        <form>
            <header>
            </header>
            <sheet>
                <div name="div_smart_buttons" class="oe_button_box" />
                <group name="group_1" colspan="4" col="2">
                    <field
                            name="allowed_field_ids"
                            widget="many2many_tags"
                            invisible="1"
                        />
                    <field name="field_id" domain="[('id', 'in', allowed_field_ids)]" />
                </group>
                <notebook>
                    <page name="python_code" string="Python Code">
                        <field
                                name="python_code"
                                colspan="4"
                                nolabel="1"
                                widget="ace"
                            />
                    </page>
                </notebook>
            </sheet>
        </form>
    </field>
</record>
</odoo>
