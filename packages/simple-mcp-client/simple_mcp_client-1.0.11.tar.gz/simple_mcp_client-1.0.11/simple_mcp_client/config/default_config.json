{
  "llm": {
    "provider": "openai",
    "model": "grok-3-mini",
    "_model": "qwen3-235b-a22b",
    "api_url": "https://api.zhizengzeng.com/v1",
    "api_key": "sk-",
    "other_params": {
      "temperature": 0.1,
      "max_tokens": 4096,
      "max_loop_iterations": 15,
      "request_timeout": 60.0
    }
  },
  "prompts": {
    "base_introduction": "You are TravelGenie, an AI assistant specialized in travel planning, destination recommendations, and trip logistics. You have expert knowledge of global tourism trends, cultural insights, transportation options, accommodations, and must-see attractions. Your goal is to provide personalized, practical, and inspiring travel advice to help users create unforgettable journeys."
  },
  "console": {
    "tool_formatting": {
      "enabled": true,
      "color": true,
      "compact": false,
      "max_depth": 3,
      "truncate_length": 300,
      "syntax_highlighting": true,
      "align_columns": true,
      "show_icons": true,
      "color_scheme": "light"
    }
  },
  "logging": {
    "log_path": null
  },
  "mcpServers": {
    "lcu": {
      "enable": false,
      "type": "sse",
      "url": "http://127.0.0.1:8012/sse",
      "command": null,
      "args": [],
      "env": {}
    },
    "weather": {
      "enable": false,
      "type": "sse",
      "url": "http://127.0.0.1:8010/sse",
      "command": null,
      "args": [],
      "env": {}
    },
    "tavily": {
      "enable": true,
      "type": "stdio",
      "url": "",
      "command": "npx",
      "args": [
        "-y",
        "tavily-mcp@0.1.4"
      ],
      "env": {
        "TAVILY_API_KEY": "tvly-dev-"
      }
    },
    "gaode": {
      "enable": false,
      "type": "sse",
      "url": "https://mcp.amap.com/sse?key=",
      "command": "",
      "args": [],
      "env": {}
    },
    "sequential-thinking": {
      "enable": true,
      "type": "stdio",
      "url": "",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "Jira": {
      "enable": false,
      "type": "stdio",
      "url": "",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "CONFLUENCE_URL",
        "-e",
        "CONFLUENCE_USERNAME",
        "-e",
        "CONFLUENCE_API_TOKEN",
        "-e",
        "JIRA_URL",
        "-e",
        "JIRA_USERNAME",
        "-e",
        "JIRA_API_TOKEN",
        "ghcr.io/sooperset/mcp-atlassian:latest"
      ],
      "env": {
        "CONFLUENCE_URL": "https://your-company.atlassian.net/wiki",
        "CONFLUENCE_USERNAME": "your.email@company.com",
        "CONFLUENCE_API_TOKEN": "your_confluence_api_token",
        "JIRA_URL": "https://your-company.atlassian.net",
        "JIRA_USERNAME": "your.email@company.com",
        "JIRA_API_TOKEN": "your_jira_api_token"
      }
    }
  }
}