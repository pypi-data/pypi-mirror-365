{
  "$schema": "https://mintlify.com/docs.json",
  "name": "Kubiya",
  "logo": {
    "dark": "https://hrkldgcnsxl9tiki.public.blob.vercel-storage.com/favicon-2-X8NQW2G1yJ778HqbzNjq0JGJrAPSmU.ico",
    "light": "https://hrkldgcnsxl9tiki.public.blob.vercel-storage.com/favicon-2-X8NQW2G1yJ778HqbzNjq0JGJrAPSmU.ico"
  },
  "favicon": "/assets/favicon.png",
  "colors": {
    "primary": "#1f2937",
    "light": "#6b7280",
    "dark": "#111827"
  },
  "topbarLinks": [
    {
      "name": "Platform",
      "url": "https://app.kubiya.ai"
    },
    {
      "name": "GitHub",
      "url": "https://github.com/kubiyabot/workflow_sdk"
    }
  ],
  "topbarCtaButton": {
    "name": "Get Started",
    "url": "https://app.kubiya.ai",
    "style": "roundedRectangle"
  },


  "anchors": [
    {
      "name": "Blog",
      "icon": "newspaper",
      "url": "https://kubiya.ai/blog"
    },
    {
      "name": "Changelog",
      "icon": "clock",
      "url": "/changelog"
    }
  ],
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude"
    ]
  },
  "api": {
    "mdx": {
      "server": ["https://api.kubiya.ai/api/v1", "http://localhost:8001"],
      "auth": {
        "method": "key",
        "name": "Authorization",
        "inputPrefix": "UserKey "
      }
    },
    "playground": {
      "mode": "show"
    }
  },
  "navigation": {
