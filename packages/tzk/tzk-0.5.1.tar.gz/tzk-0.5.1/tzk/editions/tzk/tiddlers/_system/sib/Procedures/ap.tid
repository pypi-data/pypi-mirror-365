\procedure ap(refnum)
	[[ap|attopublish]]&nbsp;
	<$m2.extlink
		urltemplate="https://ap.sorenbjornstad.com/#post-$1$"
		slotvalue=<<refnum>>
		text=<<refnum>>
	/>
\end